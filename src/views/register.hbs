<section class="section">
    <h2>Register</h2>
    <form id="register" class="form">
        <label for="name">Nombre</label>
        <input id="name" type="text" placeholder="Ingrese su nombre" name="first_name">
        <label for="last_name">Apellido</label>
        <input id="last_name" type="text" placeholder="Ingrese su apellido" name="last_name">
        <label for="email">Email</label>
        <input id="email" type="email" placeholder="Ingrese su email" name="email">
        <label for="age">Edad</label>
        <input id="age" type="number" placeholder="Ingrese su edad" name="age">

        <label for="password">Password</label>
        <input id="password" type="password" placeholder="Ingrese su email" name="password">
        <input class="button" type="submit" value="Register">

        <p>Ya tienes una cuenta? <a href="http://localhost:8080/users/login">haz click aqui</a></p>
    </form>
</section>

<script src="/js/register.js"></script>